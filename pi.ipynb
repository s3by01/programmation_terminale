    "def W(n):\n",
    "    w=1\n",
    "    k=1\n",
    "    while k<=2*n-1:\n",
    "        w=w*(k/(k+1))**2\n",
    "        k=k+2\n",
    "    w=w*(2*n+1)\n",
    "    return w\n",
    "\n",
    "pi=2*1/W(100000000)\n",
    "print (pi)"
   ]
